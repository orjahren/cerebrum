#: Cerebrum/modules/pwcheck/dictionary.py:261
msgid "Must not contain dictionary words"
msgstr "Må ikke inneholde ord som du finner i ordliste"

#: Cerebrum/modules/pwcheck/dictionary.py:272
#: Cerebrum/modules/pwcheck/dictionary.py:283
msgid "Password cannot contain dictionary words"
msgstr "Passordet må ikke inneholde ord fra ordliste"

#: Cerebrum/modules/pwcheck/dictionary.py:278
msgid "You should not combine two words like {word1} and {word2}"
msgstr "Du kan ikke kombinere to ord som {word1} og {word2}"

#: Cerebrum/modules/pwcheck/history_checks.py:33
msgid "Must not be too similar to an old password"
msgstr "Må ikke ligne på et tidligere passord"

#: Cerebrum/modules/pwcheck/history_checks.py:42
msgid "Password too similar to an old password"
msgstr "Passordet ligner mye på et tidligere passord"

#: Cerebrum/modules/pwcheck/history_checks.py:51
msgid "Must not be the same as an old password"
msgstr "Må ikke være det samme som et tidligere passord"

#: Cerebrum/modules/pwcheck/history_checks.py:60
msgid "Password is the same as an old password"
msgstr "Passordet er det samme som et tidligere passord"

#: Cerebrum/modules/pwcheck/phrase.py:35
msgid "Must contain at least {min_words} words"
msgstr "Må bestå av minst {min_words} ord"

#: Cerebrum/modules/pwcheck/phrase.py:38
msgid ""
"Must contain at least {min_words} words with a minimum length of "
"{min_word_length} characters each"
msgstr ""
"Må bestå av minst {min_words} ord med en lengde på minimum {min_word_length} "
"tegn hver"

#: Cerebrum/modules/pwcheck/phrase.py:50
msgid "Password must have at least {wds} words of length {word_length}"
msgstr "Passordet må bestå av minst {wds} ord av lengde {word_length}"

#: Cerebrum/modules/pwcheck/phrase.py:61
msgid "Words must be in average at least {avg_length} characters long"
msgstr ""
"Ordene i passordfrasen må tilsammen ha en gjennomsnittlig lengde på minimum "
"{avg_length} tegn"

#: Cerebrum/modules/pwcheck/phrase.py:69
msgid "Password words must be in average at least {avg} characters long"
msgstr "Ord i passordfrasen ha gjennomsnittlig lendge av {avg} tegn"

#: Cerebrum/modules/pwcheck/simple.py:61
msgid "Must not contain a space or the special null character"
msgstr "Må ikke inneholde mellomrom eller et tomt (null) tegn"

#: Cerebrum/modules/pwcheck/simple.py:63
msgid "Password cannot contain the null character"
msgstr "Passordet kan ikke inneholde tomme (null) tegn"

#: Cerebrum/modules/pwcheck/simple.py:64
msgid "Password cannot contain space"
msgstr "Passordet kan ikke inneholde mellomrom (space)"

#: Cerebrum/modules/pwcheck/simple.py:81
msgid "Must not contain 8-bit characters"
msgstr "Må ikke inneholde 8-bit-tegn"

#: Cerebrum/modules/pwcheck/simple.py:86
msgid "Password cannot contain 8-bit characters"
msgstr "Passordet må ikke inneholde 8-bit tegn"

#: Cerebrum/modules/pwcheck/simple.py:96
msgid "Can contain only a-z, A-Z, digits, and: {special_characters}"
msgstr "Kan inneholde kun a-z, A-Z, siffer og: {special_characters}"

#: Cerebrum/modules/pwcheck/simple.py:116
#: Cerebrum/modules/pwcheck/simple.py:176
msgid "Password can not contain the character: {character}"
msgstr "Passordet kan ikke inneholde tegnet: {character}"

#: Cerebrum/modules/pwcheck/simple.py:130
msgid "Can contain only latin1 (ISO-8859-1) compatible characters"
msgstr "Kan bare inneholde tegn fra latin 1 (ISO-8859-1)"

#: Cerebrum/modules/pwcheck/simple.py:141
msgid ""
"Password contains one or more characters that are not latin1 (ISO-8859-1) "
"compatible"
msgstr ""
"Passordet inneholder ett eller flere tegn som ikke finnes i latin1 "
"(ISO-8859-1)"

#: Cerebrum/modules/pwcheck/simple.py:161
msgid ""
"Can not contain one or more of the following characters: {illegal_characters}"
msgstr ""
"Kan ikke inneholde ett eller flere av følgende tegn: {illegal_characters}"

#: Cerebrum/modules/pwcheck/simple.py:201
msgid ""
"Must contain at least {min_chars_per_group} character(s) for each of at "
"least {min_groups} of the following character groups: Uppercase letters, "
"lowercase letters, numbers and special characters"
msgstr ""
"Må bestå av minst {min_chars_per_group} tegn for hver av minst {min_groups} "
"av de følgende tegngruppene: Store bokstaver, små bokstaver, tall, "
"spesialtegn"

#: Cerebrum/modules/pwcheck/simple.py:219
msgid ""
"Password must contain at least {min_chars_per_group} character(s) for each "
"of at least {min_groups} of the following character groups: Uppercase "
"letters, lowercase letters, numbers and special characters"
msgstr ""
"Passordet må bestå av minst {min_chars_per_group} tegn for hver av minst "
"{min_groups} av de følgende tegn-gruppene: Store bokstaver, små bokstaver, "
"tall, spesialtegn"

#: Cerebrum/modules/pwcheck/simple.py:257
msgid ""
"Password must evaluate to at least {min_required_entropy} entropy points"
msgstr ""
"Passordet må ha et kompleksitetsnivå på minst {min_required_entropy} poeng"

#: Cerebrum/modules/pwcheck/simple.py:285
msgid ""
"Password has only {entropy_value} entropy points out of "
"{min_required_entropy}"
msgstr ""
"Passordet har kompleksitetsnivå på kun {entropy_value} av "
"{min_required_entropy} poeng"

#: Cerebrum/modules/pwcheck/simple.py:319
msgid "Must be at least {min_length} characters"
msgstr "Må ha en lengde på minst {min_length} tegn"

#: Cerebrum/modules/pwcheck/simple.py:323
msgid "Must be at least {min_length} and at most {max_length} characters"
msgstr "Må være minst {min_length} og høyst {max_length} tegn langt"

#: Cerebrum/modules/pwcheck/simple.py:336
msgid "Password must be at least {min_length} characters long"
msgstr "Passordet må være minst {min_length} tegn langt"

#: Cerebrum/modules/pwcheck/simple.py:342
msgid "Password must be at most {max_length} characters long"
msgstr "Passordet må være høyst {max_length} tegn langt"

#: Cerebrum/modules/pwcheck/simple.py:354
msgid ""
"Must contain characters from at least 3 of the following character groups in "
"the first 8 characters: Uppercase letters, lowercase letters, numbers and "
"special characters. If the password only contains one uppercase letter, it "
"cannot be the first character. If the password only contains one number or "
"special character, it cannot be in position 8"
msgstr ""
"Må bestå av tegn fra minst 3 av de følgende tegngruppene blant de 8 første "
"tegnene: Store bokstaver, små bokstaver, tall, spesialtegn. Om passordet kun "
"inneholder en stor bokstav, kan ikke denne være første tegn. Om passordet "
"kun inneholder ett nummer eller ett spesialtegn, kan ikke dette være tegn nr "
"8 i passordet"

#: Cerebrum/modules/pwcheck/simple.py:404
msgid ""
"A password that only contains one uppercase letter, must not have this as "
"the first character. If the first 8 characters only contains one number or "
"special character, this must not be in position 8"
msgstr ""
"Passord som inneholder bare én stor bokstav, kan ikke ha den bokstaven som "
"første bokstav i passordet. Hvis de 8 første tegn inneholder bare ett tall "
"eller spesialtegn, kan det ikke være i posisjon 8"

#: Cerebrum/modules/pwcheck/simple.py:410
msgid ""
"The first eight characters of the password must contain characters from at "
"least three of the four following character groups: Uppercase letters, "
"lowercase letters, numbers and special characters"
msgstr ""
"De første 8 tegn i passordet må bestå av tegn av minst tre av de fire "
"tegngruppene: Store bokstaver, små bokstaver, tall, spesialtegn"

#: Cerebrum/modules/pwcheck/simple.py:422
msgid "Must not contain sequences of closely related characters"
msgstr "Må ikke inneholde sekvenser av relaterte tegn"

#: Cerebrum/modules/pwcheck/simple.py:445
msgid "Password cannot contain characters in alpabetical or numerical order"
msgstr ""
"Passordet må ikke inneholde tegn i alfabetisk eller numerisk rekkefølge"

#: Cerebrum/modules/pwcheck/simple.py:474
msgid "Password cannot contain neighbouring keyboard keys"
msgstr ""
"Passordet må ikke inneholde sekvenser av tegn som er geometrisk relatert på "
"tastaturet (f.eks. qwerty)"

#: Cerebrum/modules/pwcheck/simple.py:485
msgid "Must not contain repeated sequences of characters"
msgstr "Må ikke inneholde gjentagende sekvenser"

#: Cerebrum/modules/pwcheck/simple.py:493
msgid "Password cannot contain repeated sequences of characters"
msgstr "Passordet må ikke inneholde gjentagende sekvenser"

#: Cerebrum/modules/pwcheck/simple.py:513
msgid "Must not contain your username"
msgstr "Passordet må ikke inneholde brukernavnet ditt"

#: Cerebrum/modules/pwcheck/simple.py:526
#: Cerebrum/modules/pwcheck/simple.py:548
msgid "Password cannot contain your username"
msgstr "Passordet må ikke inneholde brukernavnet ditt"

#: Cerebrum/modules/pwcheck/simple.py:535
msgid "Must not contain your username, even in reverse"
msgstr "Må ikke inneholde brukernavnet ditt, ikke engang baklengs"

#: Cerebrum/modules/pwcheck/simple.py:552
msgid "Password cannot contain your username in reverse"
msgstr "Passordet må ikke inneholde brukernavnet ditt (baklengs)"

#: Cerebrum/modules/pwcheck/simple.py:560
msgid "Must not contain words from your fullname"
msgstr "Må ikke inneholde ord fra navnet ditt"

#: Cerebrum/modules/pwcheck/simple.py:580
#: Cerebrum/modules/pwcheck/simple.py:660
msgid "Password cannot contain your name"
msgstr "Passordet må ikke inneholde navnet ditt"

#: Cerebrum/modules/pwcheck/simple.py:590
msgid "Must not contain {name_seq_len} or more characters from your name"
msgstr "Må ikke inneholde {name_seq_len} eller flere tegn fra navnet ditt"

#: Cerebrum/modules/pwcheck/simple.py:674
msgid "Password cannot contain {seqlen} characters from your name"
msgstr "Passordet må ikke inneholde {seqlen} eller flere tegn fra ditt navn"

#: Cerebrum/modules/pwcheck/simple.py:685
msgid "Must only contain letters and spaces"
msgstr "Kan bare inneholde bokstaver og mellomrom"

#: Cerebrum/modules/pwcheck/simple.py:700
msgid "Password can only contain letters and spaces"
msgstr "Passordet kan kun inneholde bokstaver og mellomrom"

#: Cerebrum/modules/pwcheck/simple.py:709
msgid "Must contain at least {digits} digits"
msgstr "Må inneholde minst {digits} siffer"

#: Cerebrum/modules/pwcheck/simple.py:715
msgid "Password must contain at least {digits} digits"
msgstr "Passordet må inneholde minst {digits} siffer"

#: Cerebrum/modules/pwcheck/simple.py:725
msgid "Must contain at least {letters} letters"
msgstr "Må inneholde minst {letters} bokstaver"

#: Cerebrum/modules/pwcheck/simple.py:732
msgid "Password must contain at least {letters} letters"
msgstr "Passordet må inneholde minst {letters} bokstaver"

#: Cerebrum/modules/pwcheck/simple.py:741
msgid "Must contain upper and lowercase letters"
msgstr "Må inneholde store og små bokstaver"

#: Cerebrum/modules/pwcheck/simple.py:748
msgid "Password must contain upper and lowercase letters"
msgstr "Passordet må inneholde store og små bokstaver"
